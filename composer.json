{
    "name": "rdmtr/symfony-telegram-console",
    "description": "Symfony Application Console interface interaction using Telegram Bot",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Rusak",
            "email": "rusakdm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rdmtr\\TelegramConsole\\":  ""
        }
